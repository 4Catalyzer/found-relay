{
  "name": "found-relay",
  "version": "0.4.1",
  "description": "Relay integration for found",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src -d lib",
    "build-fixtures": "npm run update-schema && npm run relay-compiler",
    "format": "eslint --fix . && npm run prettier -- --write",
    "lint": "eslint . && npm run prettier -- -l",
    "prepublish": "npm run build",
    "prettier": "prettier --ignore-path .eslintignore '**/*.md'",
    "relay-compiler": "relay-compiler --watchman false --src test --schema test/fixtures/schema.graphql",
    "tdd": "jest --watch",
    "test": "npm run build-fixtures && npm run lint && npm run testonly -- --coverage",
    "testonly": "jest --runInBand --verbose",
    "update-schema": "babel-node test/fixtures/updateSchema.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "/README.md": [
      "doctoc",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**"
    ],
    "resetMocks": true,
    "restoreMocks": true,
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relay-tools/found-relay.git"
  },
  "keywords": [
    "router",
    "relay",
    "react"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relay-tools/found-relay/issues"
  },
  "homepage": "https://github.com/relay-tools/found-relay#readme",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.5.5",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "found": ">=0.3.21",
    "react": ">=16.5.0",
    "react-relay": ">=2.0.0"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.0.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-relay": "^5.0.0",
    "codecov": "^3.5.0",
    "delay": "^4.3.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.1.0",
    "eslint-config-4catalyzer-jest": "^2.0.0",
    "eslint-config-4catalyzer-react": "^1.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "farce": "^0.2.8",
    "found": "^0.4.7",
    "graphql": "^14.4.2",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "p-defer": "^3.0.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-relay": "^5.0.0",
    "relay-compiler": "^5.0.0",
    "relay-local-schema": "^0.8.0",
    "relay-runtime": "^5.0.0",
    "rimraf": "^3.0.0"
  }
}
