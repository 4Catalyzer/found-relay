{
  "name": "found-relay",
  "version": "0.3.0-alpha.13",
  "description": "Relay integration for found",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src -d lib",
    "build-fixtures": "npm run update-schema && npm run relay-compiler",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "relay-compiler": "relay-compiler --src test --schema test/fixtures/schema.graphql",
    "tdd": "jest --watch",
    "test": "npm run build-fixtures && npm run lint && npm run testonly -- --coverage",
    "testonly": "jest --runInBand --verbose",
    "update-schema": "babel-node test/fixtures/updateSchema.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**"
    ],
    "setupFiles": [
      "babel-polyfill",
      "<rootDir>/test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relay-tools/found-relay.git"
  },
  "keywords": [
    "router",
    "relay",
    "react"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relay-tools/found-relay/issues"
  },
  "homepage": "https://github.com/relay-tools/found-relay#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "prop-types-extra": "^1.0.1",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "found": ">=0.3.8",
    "react": "^0.14.9 || >=15.3.0",
    "react-relay": ">=1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-relay": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.0.0",
    "delay": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-4catalyzer-react": "^0.3.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "farce": "^0.2.4",
    "found": "^0.3.8",
    "graphql": "^0.12.3",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "p-defer": "^1.0.0",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-relay": "^1.4.1",
    "relay-compiler": "^1.4.1",
    "relay-local-schema": "^0.6.2",
    "relay-runtime": "^1.4.1",
    "rimraf": "^2.6.2"
  }
}
